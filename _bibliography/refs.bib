keywords = {business model,china,online game,virtual world},
number = {2},
pages = {130--144},
publisher = {Elsevier},
title = {{Business models and operational issues in the Chinese online game industry}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0736585306000190},
